{
  "name": "dsa-contest-rooms",
  "displayName": "DSA Contest Rooms - Competitive Programming Helper",
  "description": "Contest-style collaborative DSA practice with HackerRank integration, mock judge, and smart test generation",
  "version": "1.0.0",
  "publisher": "dsa-publisher",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruthwik000/roomcode"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Education"
  ],
  "keywords": [
    "competitive programming",
    "dsa",
    "contest",
    "hackerrank",
    "algorithms",
    "coding practice",
    "test runner"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dsaRoom.createRoom",
        "title": "DSA: Create Contest Room",
        "icon": "$(organization)"
      },
      {
        "command": "dsaRoom.joinRoom",
        "title": "DSA: Join Contest Room",
        "icon": "$(sign-in)"
      },
      {
        "command": "dsaRoom.runCode",
        "title": "DSA: Run Code",
        "icon": "$(play)"
      },
      {
        "command": "dsaRoom.startContest",
        "title": "DSA: Start Contest",
        "icon": "$(debug-start)"
      },
      {
        "command": "dsaRoom.fetchRandomProblem",
        "title": "DSA: Fetch Random Problem",
        "icon": "$(symbol-misc)"
      },
      {
        "command": "dsaRoom.fetchProblem",
        "title": "DSA: Fetch Problem by URL",
        "icon": "$(cloud-download)"
      },
      {
        "command": "dsaRoom.runTests",
        "title": "DSA: Run Sample Tests",
        "icon": "$(beaker)"
      },
      {
        "command": "dsaRoom.generateTests",
        "title": "DSA: Generate & Run More Tests",
        "icon": "$(sparkle)"
      },
      {
        "command": "dsaRoom.submitSolution",
        "title": "DSA: Submit Solution",
        "icon": "$(cloud-upload)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dsa-contest",
          "title": "DSA Contest",
          "icon": "$(code)"
        }
      ]
    },
    "views": {
      "dsa-contest": [
        {
          "type": "webview",
          "id": "dsaContestView",
          "name": "Contest Room"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run copy-js",
    "copy-js": "node -e \"require('fs').cpSync('src', 'out', {recursive: true, filter: (src) => src.endsWith('.js')})\"",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "ably": "^1.2.48",
    "axios": "^1.6.0",
    "axios-cookiejar-support": "^6.0.5",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^17.2.3",
    "tough-cookie": "^4.1.3"
  }
}
